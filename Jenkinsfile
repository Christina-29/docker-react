pipeline {
    
    agent any 
    
    stages {
        
        stage("build") {
           
            steps {
                echo 'building thie app'
                echo 'application built'
                
            }
        }
      stage("test") {
        
            steps {
                echo 'testing thie app'
            }
        }
      stage("deploy") {
        
            steps {
                echo 'deploying thie app'
            }
       }
    }
}
